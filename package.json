{
  "name": "@infineon/infineon-icons",
  "version": "1.0.2",
  "description": "Infineon Icons",
  "homepage": "https://github.com/Infineon/Infineon-Icons#readme",
  "bugs": {
    "url": "https://github.com/Infineon/Infineon-Icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Infineon/Infineon-Icons.git"
  },
  "author": "Kai Werther",
  "scripts": {
    "build": "npm run generate:js && npm run build:lib && npm run build:icons",
    "generate:js": "node ./build/generate_js",
    "build:lib": "rollup ./library/library.js --file ./dist/library.js --format es  --name \"library\"",
    "build:icons": "rollup ./generated_js/index.js --file ./dist/icons.js --format es  --name \"svgs\""
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/icons.js",
  "files": [
    "dist",
    "library/package.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "rollup": "^2.79.1",
    "auto": "^10.37.4"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  }
}
